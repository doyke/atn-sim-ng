    network-config {
	hostname trackserver
	!
	interface eth0
	 ip address 10.0.0.18/24
	!
    }
    canvas c1
    iconcoords {949.0 42.0}
    labelcoords {949.0 74.0}
    interface-peer {eth0 n18}
    custom-config {
	custom-config-id service:TrackServer
	custom-command TrackServer
	config {
	files=('track_server.cfg', )
	}
    }
    custom-config {
	custom-config-id service:TrackServer:track_server.cfg
	custom-command track_server.cfg
	config {
	[Database]
	; db_host = 172.17.255.254
	; db_name = atn_sim
	; db_user = atn_sim
	; db_pass = atn_sim
	
	[Tracks]
	exercise = DEMO1970
	
	
	}
    }
    services {DefaultRoute SSH TrackServer}
    hidden 1
}

