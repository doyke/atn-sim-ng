node n4 {
    type router
    model ground-station
    network-config {
	hostname n4
	!
	interface eth1
	 ip address 10.0.1.3/24
	 ipv6 address 2001:1::3/64
	!
	interface eth0
	 ipv6 address 2001:0::4/128
	 ip address 10.0.0.4/24
	!
    }
    canvas c1
    iconcoords {438.0 311.0}
    labelcoords {438.0 343.0}
    interface-peer {eth0 n31}
    services {zebra OSPFv3MDR vtysh IPForward GPSd Dump1090 AdsbIn}
    interface-peer {eth1 n18}
    custom-config {
	custom-config-id service:AdsbIn
	custom-command AdsbIn
	config {
	files=('adsbin.cfg', 'adsbin.sh', )
	}
    }
    custom-config {
	custom-config-id service:AdsbIn:adsbin.cfg
	custom-command adsbin.cfg
	config {
	[General]
	id = n4
	destinations = Dump1090 MySQL
	
	[Dump1090]
	type = dump1090
	server = localhost
	port = 30001
	
	[MySQL]
	type   = mysql
	dbname = mlat
	dbuser = mlat
	dbpass = mlat
	server = 172.17.255.254
	
	}
    }
}

node n31 {
    type wlan
    network-config {
	hostname wlan31
	!
	interface wireless
	 ipv6 address 2001:0::0/128
	 ip address 10.0.0.0/24
	!
	mobmodel
	coreapi
	emane_rfpipe
	!
    }
    custom-config {
	custom-config-id emane_rfpipe
	custom-command {11 3 11 2 10 9 9 11 9 9 4 4 4 2 9 9 9 11 10 4 10}
	config {
	enablepromiscuousmode=0
	datarate=1M
	flowcontrolenable=0
	flowcontroltokens=10
	pcrcurveuri=/usr/share/emane/xml/models/mac/rfpipe/rfpipepcr.xml
	jitter=0.0
	delay=0.0
	radiometricenable=0
	radiometricreportinterval=1.0
	neighbormetricdeletetime=60.0
	bandwidth=1M
	frequency=1.090G
	frequencyofinterest=1.090G
	subid=1
	systemnoisefigure=4.0
	txpower=100.0
	fixedantennagain=0.0
	fixedantennagainenable=1
	noisemode=none
	noisebinsize=20
	propagationmodel=freespace
	}
    }
    custom-config {
	custom-config-id basic_range
	custom-command {3 3 9 9 9}
	config {
	range=275
	bandwidth=54000000
	jitter=0
	delay=20000
	error=0
	}
    }
    canvas c1
    iconcoords {881.0 82.0}
    labelcoords {881.0 114.0}
    interface-peer {e3 n4}
    interface-peer {e2 n3}
    interface-peer {e9 n10}
    interface-peer {e13 n14}
    interface-peer {e19 n20}
    interface-peer {e0 n1}
    interface-peer {e1 n2}
    interface-peer {e4 n5}
    interface-peer {e12 n13}
    interface-peer {e15 n16}
}

