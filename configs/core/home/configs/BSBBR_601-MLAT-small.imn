	#!/bin/sh
	# auto-generated by Spoofer (spoofer.py)
	# python /opt/adsb/spoofer.py --dest=10.0.0.255 -b
	
	}
    }
    services {DefaultRoute AdsbIn Spoofer Flooder}
}

